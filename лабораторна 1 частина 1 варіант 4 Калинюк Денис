#include <iostream>

#include <stdlib.h>
using namespace std;

int main()
{
    const int MAX  = 20;
int A[MAX], B[MAX];
int i,j;
int t;
std::cout <<  " \n A  \n" << std::endl;
for (i=0; i<MAX; i++)
   {A[i] = 500 - rand()% 1000;
   cout << "  " << A[i];
}


j=0;
for (i=0; i<MAX; i++)
  if (A[i] < 0) B[j++] = A[i];

 std::cout <<  " \n B  \n" << std::endl;
 
  for (i=0; i<j; i++)
  cout << " " <<B[i];
        return 0;
}